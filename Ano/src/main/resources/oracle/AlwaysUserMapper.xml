<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="always.anything.dao.AlwaysUserLogin">
    <select id="selectAlwaysUser" resultType="always.anything.entity.AlwaysUser" parameterType="java.lang.String">
        select *
        from alwaysuser
        where always=#{always}
    </select>
</mapper>